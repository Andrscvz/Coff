clase Persona{
	atributos:
		entero edad;
		entero aniosExperiencia;
		texto nombre;
	metodos:

}


clase Empleado extiende Persona{
	atributos:
		entero id;
	metodos:

}

clase Ingeniero extiende Empleado{
	atributos:
		texto carrera = "ITC";
	metodos:
		funcion vacio imprimeAtributos(){
			imprimir("Mi nombre es");
			imprimir(nombre);
			imprimir("Mi edad es");
			imprimir(edad);
			imprimir("Mi id en la empresa es");
			imprimir(id);
			imprimir("Estudie");
			imprimir(aniosExperiencia);
		}		
}

clase Abogado extiende Empleado{
	atributos:
		texto carrera = "Leyes";
	metodos:
		funcion vacio imprimeAtributos(){
			imprimir("Mi nombre es");
			imprimir(nombre);
			imprimir("Mi edad es");
			imprimir(edad);
			imprimir("Mi id en la empresa es");
			imprimir(id);
			imprimir("Estudie");
			imprimir(aniosExperiencia);
		}		
}


principal vacio inicio(){

	Abogado a;
	Ingeniero i;

	asigna i.nombre = "nombreIngeniero";


	

