
principal vacio inicio(){
	entero i = 0, j = 0, aux = 0;
	entero arr[10] = [	1,	0,	9,	2,	8,
						3,	7,	4,	6,	5];

	mientras(i < 10){
		asigna j = i;
		mientras(j < 10){
			si(arr[i] > arr[j]){
				asigna aux = arr[j];
				asigna arr[j] = arr[i];
				asigna arr[i] = aux;
			}

			asigna j = j + 1;
		}
		asigna i = i + 1;
	}

	imprimir(	arr[0],	arr[1],	arr[2],	arr[3],	arr[4],
				arr[5],	arr[6],	arr[7],	arr[8],	arr[9] );

}